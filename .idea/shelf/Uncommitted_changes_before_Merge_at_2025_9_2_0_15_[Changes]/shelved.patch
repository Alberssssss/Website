<+><project xmlns=\"http://maven.apache.org/POM/4.0.0\"\r\n         xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\"\r\n         xsi:schemaLocation=\"https://maven.apache.org/xsd/maven-4.0.0.xsd\">\r\n    <modelVersion>4.0.0</modelVersion>\r\n\r\n    <!-- 使用 Spring Boot Parent 管理版本 -->\r\n    <parent>\r\n        <groupId>org.springframework.boot</groupId>\r\n        <artifactId>spring-boot-starter-parent</artifactId>\r\n        <version>3.1.0</version>\r\n        <relativePath/> <!-- 从仓库中查找父 POM -->\r\n    </parent>\r\n\r\n    <groupId>com.example</groupId>\r\n    <artifactId>springfx-demo</artifactId>\r\n    <version>1.0.0</version>\r\n    <packaging>jar</packaging>\r\n\r\n    <properties>\r\n        <!-- Java 版本 -->\r\n        <java.version>17</java.version>\r\n        <!-- 构建时间格式 -->\r\n        <maven.build.timestamp.format>yyyy-MM-dd HH:mm:ss</maven.build.timestamp.format>\r\n    </properties>\r\n\r\n    <dependencies>\r\n        <!-- Spring Web MVC -->\r\n        <dependency>\r\n            <groupId>org.springframework.boot</groupId>\r\n            <artifactId>spring-boot-starter-web</artifactId>\r\n        </dependency>\r\n        <!-- JPA & 数据库访问 -->\r\n        <dependency>\r\n            <groupId>org.springframework.boot</groupId>\r\n            <artifactId>spring-boot-starter-data-jpa</artifactId>\r\n        </dependency>\r\n        <!-- Thymeleaf 模板引擎 -->\r\n        <dependency>\r\n            <groupId>org.springframework.boot</groupId>\r\n            <artifactId>spring-boot-starter-thymeleaf</artifactId>\r\n        </dependency>\r\n        <!-- H2 内存数据库（开发时用） -->\r\n        <dependency>\r\n            <groupId>com.h2database</groupId>\r\n            <artifactId>h2</artifactId>\r\n            <scope>runtime</scope>\r\n        </dependency>\r\n    </dependencies>\r\n\r\n    <build>\r\n        <!-- 先排除掉所有普通资源里的 version.properties -->\r\n        <resources>\r\n            <resource>\r\n                <directory>src/main/resources</directory>\r\n                <excludes>\r\n                    <exclude>version.properties</exclude>\r\n                </excludes>\r\n                <filtering>false</filtering>\r\n            </resource>\r\n            <!-- 单独把 version.properties 作为“要过滤”的资源 -->\r\n            <resource>\r\n                <directory>src/main/resources</directory>\r\n                <includes>\r\n                    <include>version.properties</include>\r\n                </includes>\r\n                <filtering>true</filtering>\r\n            </resource>\r\n        </resources>\r\n\r\n        <plugins>\r\n            <!-- Spring Boot 可执行 Jar 打包 -->\r\n            <plugin>\r\n                <groupId>org.springframework.boot</groupId>\r\n                <artifactId>spring-boot-maven-plugin</artifactId>\r\n            </plugin>\r\n\r\n            <!-- 让 Maven 在 process-resources 阶段真正替换占位符 -->\r\n            <plugin>\r\n                <artifactId>maven-resources-plugin</artifactId>\r\n                <version>3.3.1</version>\r\n                <executions>\r\n                    <execution>\r\n                        <id>filter-version</id>\r\n                        <phase>process-resources</phase>\r\n                        <goals>\r\n                            <goal>resources</goal>\r\n                        </goals>\r\n                    </execution>\r\n                </executions>\r\n            </plugin>\r\n        </plugins>\r\n    </build>\r\n</project>\r\n
